{
    "name": "Tumblr Shop Remover",
    "description": "Does what it says on the box",
    "version": "1.0",
    "manifest_version": 2,

    "content_scripts": [
        {
            "matches": ["*://*.tumblr.com/*"],
            "js": ["removeshop.js"]
        }
    ],
    "icons":{
        "48": "icons/ico48.png"
    }
}